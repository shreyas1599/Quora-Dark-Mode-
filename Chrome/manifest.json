{
	"name": "Quora Dark Mode",
	"version": "1.0",
	"description": "Dark Mode Extension for Quora",
	"manifest_version": 2,
    "permissions": ["https://*/*","activeTab","tabs"],
    "content_scripts": [{
        "matches": ["https://*.quora.com/*"],
        "css": ["style.css"],
        "all_frames": true,
        "run_at": "document_start"
    }],
    "web_accessible_resources": ["style.css"]
}


